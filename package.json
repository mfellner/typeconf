{
  "name": "typeconf",
  "version": "0.6.1",
  "description": "A typesafe hierarchical configuration manager for Node.js and the browser.",
  "browser": "dist/TypeConf.js",
  "main": "dist/TypeConfNode.js",
  "types": "./dist/TypeConfNode.d.ts",
  "repository": "https://github.com/mfellner/typeconf",
  "author": "Maximilian Fellner <max.fellner@gmail.com>",
  "license": "MIT",
  "files": [
    "dist/"
  ],
  "keywords": [
    "configuration",
    "settings",
    "typescript",
    "environment"
  ],
  "engines": {
    "node": ">=8.9.4"
  },
  "scripts": {
    "clean": "rimraf dist/*",
    "build": "tsc -p .",
    "dist": "npm run clean && npm run build",
    "lint": "tslint --type-check --project tsconfig.json",
    "test": "npm run lint && jest --coverage",
    "prettier": "prettier --print-width=100 --single-quote --write \"./src/**/*.+(js|ts)\"",
    "prepublishOnly": "npm run dist"
  },
  "dependencies": {
    "camel-case": "^3.0.0",
    "constant-case": "^2.0.0",
    "lodash.merge": "^4.6.1"
  },
  "optionalDependencies": {
    "js-yaml": "^3.10.0",
    "minimist": "^1.2.0"
  },
  "devDependencies": {
    "@types/jest": "22.1.3",
    "@types/lodash.merge": "4.6.3",
    "@types/minimist": "1.2.0",
    "@types/node": "9.4.6",
    "jest": "22.4.2",
    "prettier": "1.11.0",
    "rimraf": "2.6.2",
    "ts-jest": "22.4.0",
    "tslint": "5.9.1",
    "typescript": "2.7.2"
  },
  "jest": {
    "testEnvironment": "node",
    "moduleFileExtensions": [
      "js",
      "ts"
    ],
    "transform": {
      "\\.ts$": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": "/test/.*\\.test.ts$",
    "coverageReporters": [
      "json",
      "lcov",
      "text",
      "html"
    ]
  }
}
